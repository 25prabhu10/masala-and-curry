{
  "$schema": "https://turborepo.org/schema.json",
  "extends": ["//"],
  "tasks": {
    "dev": {
      "dependsOn": [
        "@mac/resources#dev",
        "@mac/validators#dev",
        "@mac/api-client#dev"
      ],
      "persistent": true,
      "interactive": true
    },
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["../server/public"]
    }
  }
}
