{
  "name": "masala-and-curry",
  "slug": "masala-and-curry",
  "scheme": "masala-and-curry",
  "version": "0.0.1",
  "orientation": "portrait",
  "userInterfaceStyle": "automatic",
  "icon": "./assets/icons/icon.png",
  "updates": {
    "fallbackToCacheTimeout": 0
  },
  "newArchEnabled": true,
  "jsEngine": "hermes",
  "assetBundlePatterns": ["**/*"],
  "android": {
    "icon": "./assets/icons/icon.png",
    "package": "com.masalaandcurry",
    "adaptiveIcon": {
      "foregroundImage": "./assets/icons/adaptive-icon.png",
      "monochromeImage": "./assets/icons/adaptive-icon.png",
      "backgroundColor": "#ffffff"
    },
    "allowBackup": false,
    "edgeToEdgeEnabled": true
  },
  "ios": {
    "icon": {
      "dark": "./assets/icons/ios-dark.png",
      "light": "./assets/icons/ios-dark.png",
      "tinted": "./assets/icons/ios-tinted.png"
    },
    "supportsTablet": true,
    "bundleIdentifier": "com.masalaandcurry"
  },
  "web": {
    "favicon": "./assets/icons/favicon.png",
    "bundler": "metro",
    "output": "static"
  },
  "experiments": {
    "tsconfigPaths": true,
    "typedRoutes": true
  },
  "plugins": [
    [
      "expo-splash-screen",
      {
        "image": "./assets/icons/splash-icon-dark.png",
        "imageWidth": 200,
        "resizeMode": "contain",
        "backgroundColor": "#ffffff",
        "dark": {
          "image": "./assets/icons/splash-icon-light.png",
          "backgroundColor": "#000000"
        }
      }
    ],
    [
      "expo-font",
      {
        "fonts": ["./assets/fonts/Poppins-Regular.ttf", "./assets/fonts/Poppins-Bold.ttf"],
        "android": {
          "fonts": [
            {
              "fontFamily": "Inter",
              "fontDefinitions": [
                {
                  "path": "./assets/fonts/Poppins-Regular.ttf",
                  "weight": 400,
                  "style": "normal"
                },
                {
                  "path": "./assets/fonts/Poppins-Bold.ttf",
                  "weight": 700,
                  "style": "normal"
                }
              ]
            }
          ]
        },
        "ios": {
          "fonts": ["./assets/fonts/Poppins-Regular.ttf", "./assets/fonts/Poppins-Bold.ttf"]
        }
      }
    ],
    "expo-router"
  ]
}
