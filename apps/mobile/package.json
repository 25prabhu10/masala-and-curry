{
  "name": "@mac/mobile",
  "version": "0.0.2",
  "private": true,
  "description": "Masala and Curry mobile app.",
  "keywords": [
    "react-native",
    "expo",
    "mobile",
    "react",
    "typescript",
    "nativewind"
  ],
  "license": "GPL-2.0-only",
  "author": "Prabhu K H",
  "main": "expo-router/entry",
  "scripts": {
    "android": "expo run:android",
    "assist": "biome check --write --assist-enabled=true --formatter-enabled=false --linter-enabled=false",
    "bundle:web": "expo export --platform web",
    "check-types": "tsgo --noEmit",
    "clean": "git clean -xdf .cache .expo .turbo node_modules",
    "ios": "expo run:ios",
    "lint": "oxlint . --type-aware  --tsconfig ./tsconfig.json",
    "lint:fix": "oxlint . --type-aware  --fix --tsconfig ./tsconfig.json",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "serve:web": "serve dist",
    "web": "expo start -c --web"
  },
  "dependencies": {
    "@mac/mobile-ui": "workspace:*",
    "expo": "~54.0.22",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "^6.0.17",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-localization": "~17.0.7",
    "expo-router": "~6.0.14",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.8",
    "nativewind": "catalog:mobile",
    "react": "catalog:react19",
    "react-native": "catalog:react19",
    "react-native-mmkv": "^4.0.0",
    "react-native-reanimated": "~4.1.3",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-worklets": "^0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@mac/tailwind-config": "workspace:*",
    "@mac/tsconfig": "workspace:*",
    "@tailwindcss/postcss": "latest",
    "@types/react": "catalog:react19",
    "@typescript/native-preview": "catalog:",
    "babel-preset-expo": "~54.0.6",
    "oxlint": "catalog:",
    "oxlint-tsgolint": "catalog:",
    "postcss": "latest",
    "tailwindcss": "catalog:frontend"
  },
  "packageManager": "pnpm@10.20.0"
}
