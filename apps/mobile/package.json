{
  "name": "@mac/mobile",
  "version": "0.0.1",
  "private": true,
  "description": "Masala and Curry mobile app.",
  "keywords": [],
  "license": "GPL-2.0-only",
  "author": "Prabhu K H",
  "main": "expo-router/entry",
  "scripts": {
    "android": "expo run:android",
    "assist": "biome check --write --assist-enabled=true --formatter-enabled=false --linter-enabled=false",
    "bundle:web": "expo export --platform web",
    "check-types": "tsc --noEmit",
    "clean": "git clean -xdf .cache .expo .turbo node_modules",
    "postinstall": "pnpm exec tailwindcss -i ./src/styles/global.css -o ./node_modules/.cache/nativewind/global.css",
    "ios": "expo run:ios",
    "lint": "oxlint . --tsconfig ./tsconfig.json",
    "lint:fix": "oxlint . --fix --tsconfig ./tsconfig.json",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "serve:web": "serve dist",
    "web": "expo start -c --web"
  },
  "dependencies": {
    "@mac/mobile-ui": "workspace:*",
    "expo": "~53.0.19",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "^5.2.4",
    "expo-font": "~13.3.2",
    "expo-linking": "~7.1.7",
    "expo-localization": "~16.1.6",
    "expo-router": "~5.1.3",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "nativewind": "catalog:ui",
    "react": "catalog:react19",
    "react-native": "catalog:react19",
    "react-native-mmkv": "^3.3.0",
    "react-native-reanimated": "~3.17.5",
    "react-native-safe-area-context": "~5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "catalog:mobile"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@mac/tailwind-config": "workspace:*",
    "@mac/tsconfig": "workspace:*",
    "@types/react": "catalog:react19",
    "oxlint": "catalog:",
    "tailwindcss": "catalog:ui",
    "typescript": "catalog:"
  },
  "packageManager": "pnpm@10.14.0"
}
