{
  "$schema": "../../node_modules/wrangler/config-schema.json",
  "name": "mac-server",
  "main": "./src/index.ts",
  "compatibility_date": "2025-07-09",
  "workers_dev": true,
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "directory": "public"
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "mac-db",
      "database_id": "000-000-000-000",
      "migrations_dir": "./src/db/migrations"
    }
  ]
}
