{
  "name": "@mac/mobile-ui",
  "version": "0.0.2",
  "private": true,
  "description": "Mobile UI components for React Native applications",
  "keywords": [],
  "license": "GPL-2.0-only",
  "author": "Prabhu K H",
  "type": "module",
  "exports": {
    "./button": "./src/button.tsx",
    "./text": "./src/text.tsx",
    "./icons/*": "./src/icons/*.tsx"
  },
  "scripts": {
    "check-types": "tsgo --noEmit",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "lint": "oxlint . --type-aware  --tsconfig ./tsconfig.json",
    "lint:fix": "oxlint . --type-aware  --fix --tsconfig ./tsconfig.json",
    "ui-add": "pnpm dlx @react-native-reusables/cli@latest add"
  },
  "dependencies": {
    "@rn-primitives/slot": "^1.2.0",
    "lucide-react-native": "catalog:mobile",
    "react-native-css": "catalog:mobile",
    "react-native-svg": "catalog:mobile"
  },
  "devDependencies": {
    "@mac/tailwind-config": "workspace:*",
    "@mac/tsconfig": "workspace:*",
    "@types/react": "catalog:react19",
    "@typescript/native-preview": "catalog:",
    "nativewind": "catalog:mobile",
    "oxlint": "catalog:",
    "oxlint-tsgolint": "catalog:",
    "react": "catalog:react19",
    "react-native": "catalog:react19"
  },
  "peerDependencies": {
    "react": "catalog:react19",
    "react-native": "catalog:react19"
  },
  "packageManager": "pnpm@10.20.0"
}
