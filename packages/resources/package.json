{
  "name": "@mac/resources",
  "version": "0.0.2",
  "private": true,
  "description": "Masala and Curry API - Resource Constants",
  "keywords": [],
  "license": "GPL-2.0-only",
  "author": "Prabhu K H",
  "type": "module",
  "exports": {
    "./app": {
      "types": "./dist/app.d.ts",
      "import": "./dist/app.js",
      "default": "./dist/app.js"
    },
    "./constants": {
      "types": "./dist/constants.d.ts",
      "import": "./dist/constants.js",
      "default": "./dist/constants.js"
    },
    "./category": {
      "types": "./dist/category.d.ts",
      "import": "./dist/category.js",
      "default": "./dist/category.js"
    },
    "./menu-item": {
      "types": "./dist/menu-item.d.ts",
      "import": "./dist/menu-item.js",
      "default": "./dist/menu-item.js"
    },
    "./general": {
      "types": "./dist/general.d.ts",
      "import": "./dist/general.js",
      "default": "./dist/general.js"
    },
    "./user": {
      "types": "./dist/user.d.ts",
      "import": "./dist/user.js",
      "default": "./dist/user.js"
    },
    "./http-status-codes": {
      "types": "./dist/http-status-codes.d.ts",
      "import": "./dist/http-status-codes.js",
      "default": "./dist/http-status-codes.js"
    },
    "./http-status-phrases": {
      "types": "./dist/http-status-phrases.d.ts",
      "import": "./dist/http-status-phrases.js",
      "default": "./dist/http-status-phrases.js"
    }
  },
  "scripts": {
    "build": "tsgo",
    "check-types": "tsgo --noEmit",
    "clean": "git clean -xdf dist .turbo node_modules .cache",
    "dev": "tsgo",
    "lint": "oxlint . --type-aware  --tsconfig ./tsconfig.json",
    "lint:fix": "oxlint . --type-aware  --fix --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@mac/tsconfig": "workspace:*",
    "@typescript/native-preview": "catalog:",
    "oxlint": "catalog:",
    "oxlint-tsgolint": "catalog:"
  },
  "packageManager": "pnpm@10.20.0"
}
