{
  "name": "@mac/tailwind-config",
  "version": "0.0.2",
  "private": true,
  "description": "Tailwind CSS configuration files.",
  "keywords": [
    "css",
    "tailwind"
  ],
  "license": "GPL-2.0-only",
  "author": "Prabhu K H",
  "type": "module",
  "exports": {
    ".": "./shared.css",
    "./utils": "./utils.ts"
  },
  "scripts": {
    "check-types": "tsgo --noEmit",
    "clean": "git clean -xdf .cache .turbo node_modules",
    "lint": "oxlint . --type-aware  --tsconfig ./tsconfig.json",
    "lint:fix": "oxlint . --type-aware  --fix --tsconfig ./tsconfig.json"
  },
  "dependencies": {
    "class-variance-authority": "catalog:frontend",
    "tailwind-merge": "catalog:frontend"
  },
  "devDependencies": {
    "@mac/tsconfig": "workspace:*",
    "@typescript/native-preview": "catalog:",
    "oxlint": "catalog:",
    "oxlint-tsgolint": "catalog:"
  },
  "packageManager": "pnpm@10.20.0"
}
